{
  "name": "jinaga-react",
  "version": "2.1.1",
  "description": "React binding helpers for Jinaga",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "dev": "nodemon -e ts -w ./src -x npm run test:watch",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "keywords": [
    "react",
    "viewmodel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/michaellperry/jinaga-react"
  },
  "author": "Michael L Perry",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^9.0.1",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.6",
    "jest": "^24.5.0",
    "jinaga": "^2.3.3",
    "nodemon": "^2.0.0",
    "react": "^16.8",
    "react-dom": "^16.8.3",
    "ts-jest": "^24.0.0",
    "ts-loader": "^6.0.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.2.3"
  },
  "peerDependencies": {
    "jinaga": "^2.3.1",
    "react": "^16.8"
  }
